package general;

public class Person{
	
	String name;
	public Person(String name){
		this.name = name;
	}
	
	public boolean equals(Person p2){
		if(this.hashCode() == p2.hashCode()){
			return true;
		}else {
			return false;
		}
	}
	
	public int hashCode(){
		return this.name.hashCode();
	}
	
	public String getName(){
		return this.name;
	}

	@Override

}
