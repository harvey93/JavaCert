package hackerRank;

public class Paths {
	
	public static int count(int[][] matrix){
		
		return countRec(0, new int[] {0,0}, matrix);
		
	}
	
	public static int countRec(int paths, int[] pos, int[][] matrix){
		if(pos[0] > matrix.length - 1 || pos[0] < 0){
			return;
		}else if(pos[1] > matrix[0].length - 1 || pos[1] < 0){
			return;
		}
		paths += 
		return 5;
	}

}
