package ch1;

public class Urlify {

	public static void replace(char [] arr, int trueLength){
		int i = arr.length - 1;
		int j = trueLength;
		while(j >= 0){
			if(arr[i] == " ".charAt(0) && i > trueLength - 1){
				arr[i] = arr[j];
				i -= 3;
			}else if(arr[i] == " ".charAt(0)){
				
			} else {
				
				i--;
			}
			j--;
		}
		
	}
}
