public class Sets {
	
	public static ArrayList<ArrayList<Integer>> subSets(ArrayList<Integer> list){
		if(list.size() == 0){
			return new ArrayList<ArrayList<Integer>>();
		}
		
		Integer last = list.remove(list.size() - 1);
		ArrayList<Integer> prevList = subSet(list);
		
		
	}

}
