package ch8;

public class MagicIndex {

	public static int findIndexUnique(int [] arr){
		return findIndexUnique(arr, 0, arr.length - 1);
	}
	
	public static int findIndexUnique(int [] arr, int start, int end){
		if(end < start){
			return -1;
		}
		int mid = (start + end) / 2;
		if(arr[mid] == mid){
			return mid;
		}
	}
}
