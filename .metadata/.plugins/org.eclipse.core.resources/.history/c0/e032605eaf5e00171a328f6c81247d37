package ch1;

public class OneAway {

	public static boolean isOneAway(String str1, String str2){
		System.out.println(replace(str1, str2));
		return true;
	}
	
	public static boolean replace(String str1, String str2){
		int i = 0;
		int j = 0;
		int wrongCount = 0;
		while(i < str1.length()){
			i++;
			j++;
		}
		return false;
	}
}
