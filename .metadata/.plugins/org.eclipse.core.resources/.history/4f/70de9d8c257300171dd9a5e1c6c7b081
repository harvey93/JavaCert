package hackerRank;

public class Sorting {
	
	public static void bubbleSort(int [] arr){
		for(int i = 0; i < arr.length - 1; i++){
			for(int j = i + 1; j < arr.length; j++){
				
			}
		}
	}
	
	public static void exch(int [] arr, int i, int j){
		int temp = arr[i];
		arr[i] = arr[j];
		arr[j] = arr[i];
	}
	

}
