package ch3;

import java.lang.reflect.AnnotatedArrayType;

public class DeleteMe<T> {
	
	private T data;
	private DeleteMe<T> next;
	private DeleteMe<T> prev;
	
	private Integer[] array;
	
	public DeleteMe(T el){
		this.data = el;
		this.next = null;
		this.prev = null;
	}

	public DeleteMe<T> getPrev() {
		return this.prev;
	}

	public void setPrev(DeleteMe<T> prev) {
		this.prev = prev;
	}

	public T getData() {
		return data;
	}

	public void setData(T data) {
		this.data = data;
	}

	public DeleteMe<T> getNext() {
		return next;
	}

	public void setNext(DeleteMe<T> next) {
		this.next = next;
	}
	
	public String toString(){
		String result = "";
		result += " "+ (this.data == null ? "null" : this.data);
		return result;
	}
	
	
}
