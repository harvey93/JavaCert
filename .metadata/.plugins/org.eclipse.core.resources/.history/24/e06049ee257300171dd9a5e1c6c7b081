package hackerRank;

public class Sorting {
	
	public static void bubbleSort(int [] arr){
		int times = arr.length - 1;
		while(i){
			for(int i = 0; i < arr.length - 1; i++){
				if(arr[i] > arr[i + 1]){
					exch(arr, i, i+1);
				}
			}
			times--;
		}
	}
	
	public static void exch(int [] arr, int i, int j){
		int temp = arr[i];
		arr[i] = arr[j];
		arr[j] = temp;
	}
	

}
