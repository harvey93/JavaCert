package ch3;

import java.util.*;

public class ArrStack<T> {
	
	private T[] store;
	private int i;
	
	
	public ArrStack(){
//		this.store = new T[10];
	}
	
	public void push(T el){
		store[i] = el;
		i++;
	}
	
	public T pop(){
		return this.store.remove(size() - 1);
	}
	
	public int size(){
		return this.store.size();
	}
	
	public String toString(){
		return this.store.toString();
	}
	

}
