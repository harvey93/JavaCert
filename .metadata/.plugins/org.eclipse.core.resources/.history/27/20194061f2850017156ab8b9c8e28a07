package ch4;

public class BST{
	Node root;
	
	public void insert(T data){
		Node node = new Node(data);
		if(root == null){
			root = node;
		}else {
			Node<T> current = root;
			while(current != null){
				if(node.data < current.data){
					current = current.left;
				}
				
			}
		}
	}
	
	 
	
	private class Node{
		Integer data;
		Node left;
		Node right;
		
		public Node(Integer data){
			this.data = data;
		}
	}
	
	

}
