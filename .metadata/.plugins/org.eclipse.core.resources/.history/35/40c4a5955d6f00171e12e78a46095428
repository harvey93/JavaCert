package hackerRank;

import java.awt.List;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;

public class StringProbs {

	public int numbersNeeded(String first, String second){
		ArrayList<String> list1 = new ArrayList<>(Arrays.asList(first.split("")));
		ArrayList<String> list2 = new ArrayList<>(Arrays.asList(second.split("")));
		System.out.println(list1);
		System.out.println(list2);
		return 5;
	}
}
