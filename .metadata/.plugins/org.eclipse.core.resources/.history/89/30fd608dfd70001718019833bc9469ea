package sectionTests;

public enum Color {
	Red("RED"), Blue("BLUE"), Green("GREEN");

	String color = null;

	Color(String color) {
		this.color = color;
	}
	
	public int getColorCode(Color color) {
		int ret = -1;
		switch (color) {
		case Red:
			ret = 1;
			break;

		default:
			break;
		}
	}
}
