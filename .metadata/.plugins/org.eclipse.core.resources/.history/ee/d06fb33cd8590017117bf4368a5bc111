package aAProbs;

public class Vowels {
	public static int countVowels(String str) {
		for(int i = 0; i < str.length(); i++) {
			if(isVowel(str.charAt(i))){
				
			}
		}
	}
	
	public static boolean isVowel(char chr){
			if((chr == 'a') || (chr == 'e') || (chr == 'i') || (chr == 'o') || (chr == 'u')){
				return true;	
			} else {
				return false;
			}
			
	}
}
