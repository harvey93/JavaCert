package ch3;

import java.lang.reflect.Array;
import java.util.Arrays;

public class MyStack<E> {
	
	private E[] store;
	
	public MyStack(){
		this.store = (E[])Array.newInstance(componentType, length);
		this.index = 0;
	}
	
	public void pop(E el){

		add(el);
	}
	
	public int size(){
		return this.store.length;
	}
	
	public void add(E el){
		this.store[this.index] = el;
		index++;
	}
	
}
