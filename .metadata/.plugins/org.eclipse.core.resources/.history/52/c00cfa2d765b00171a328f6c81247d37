package aAProbs;
import java.util.*;

public class Primes {

	public static boolean isPrime(int num){
		double dubNum = (double ) num;
		boolean retVal = true;
		for(int i = 2; i <= Math.sqrt(dubNum); i++){
			if(num % i == 0){
				retVal = false;
				break;
			}
		}
		return retVal;
	}
	
	public static int nthPrime(int num){
		if(num == 1){
			return 2;
		}
		ArrayList<Integer> arr = new ArrayList<>();
		arr.add(1);
		arr.add(2);
		
		return arr.get(arr.size() - 1);
	}
}
