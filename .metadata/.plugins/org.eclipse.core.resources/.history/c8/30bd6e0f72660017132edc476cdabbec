package ch2;

public class Node {
	private Integer data;
	private Node next;
	private Node prev;
	
	public Node(Integer data){
		this.data = data;
	}
	
	public Node getNext(){
		return this.next;
	}
	
	public void setNext(Node next){
		this.next = next;
	}
	
	public Integer getData(){
		return this.data;
	}
	
	public void setData(Integer data){
		this.data = data;
	}
	
	public Node getPrev(){
		return this.prev;
	}
	
	public void setPrev(Node prev){
		this.prev = prev;
	}
}
