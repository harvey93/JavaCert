package ch8;

import java.util.Comparator;

public class Boxes {

}

class Box{
	Integer h;
	Integer w;
	Integer d;
	
	public Box(int h, int w, int d){
		this.h = h;
		this.w = w;
		this.d = d;
	}
}

class BoxComparator implements Comparator<Box>{

	
	@Override
	public int compare(Box o1, Box o2) {
		// TODO Auto-generated method stub
		return -1 * o1.h.compareTo(o2.h);
	}
}