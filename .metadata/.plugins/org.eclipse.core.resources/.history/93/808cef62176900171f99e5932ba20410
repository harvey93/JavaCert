package general;

public class Person{
	
	String name;
	public Person(String name){
		this.name = name;
	}
	
	public boolean equals(Person p2){
//		if(this.name.hashCode() == p2.getName().hashCode()){
//			return true;
//		}else {
//			return false;
//		}
		if(this.name == p2.getName()){
			return true;
		}
	}
	
	public int hashCode(){
		return this.name.hashCode();
	}
	
	public String getName(){
		return this.name;
	}

}
