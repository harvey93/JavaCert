package concurrency;

public class Race {
	
	Runner winner;
	public void start(Runner runner){
		
		try{
			int random = (int)(Math.random() * 100);
			Thread.sleep(random);
		}catch(InterruptedException e){
			System.out.println(e.getMessage());
		}
		
		
		
		if(winner == null){
			System.out.println(runner.name);
			winner = runner;
		}
		
	}
}
