package aAProbs;
import java.util.*;
public class Palindrome {
	public static boolean isPalindrome(String str){
		if(str.equals(StringReverse.reverseIP(str))){
			return true;
		}else {
			return false;
		}
	}
	
	public static String longestPalindrome(String str) {
		ArrayList<String> arr = ArrayList<>();
		return str;
	}
}
