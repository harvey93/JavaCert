package ch3;

import java.util.ArrayList;
import java.util.Stack;

public class Plates<T>{
	
	private ArrayList<Stack<T>> store;
	private int currentStack;
	
	public Plates(){
		this.store = new ArrayList<>();
	}
	
	public int size(){
		int result;
		if (currentStack == 0) {
			result =  this.store.get(this.currentStack).size();
		}
		return result
	}
	
	

}
